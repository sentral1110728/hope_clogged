<div class="content">
  <%= form_with(url: search_group_reports_path, local: true, method: :get, class: "search-form") do |form| %>
    <%= form.text_field :keyword, placeholder: "レポートの日付検索", class: "search-input" %>
    <%= form.submit "検索", class: "search-btn" %>
  <% end %>
  <% @reports.each do |report| %>
    <%= render partial: "report", locals: { report: report } %>
  <% end %>
</div>