<div class = "reports">
  <div class="reports__left">
    <%= link_to group_report_path(@group,report) do %>
      <%=report.date%>
    <% end %>
  </div>
  <div class="reports__right">
    <div class="reports__right__top">
      <%= report.group.name %>
    </div>
    <div class="reports__right__name">
      <%= report.user.name %>
    </div>
  </div>
</div>
